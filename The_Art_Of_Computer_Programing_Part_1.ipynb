    "def Euclids(m,n):\n",
    "    if m < n:\n",
    "        m,n = n,m\n",
    "    r = m % n\n",
    "    if r == 0:\n",
    "        return n\n",
    "    else:\n",
    "        return Euclids(n,r)\n",
    "\n",
    "\n",
    "print(Euclids(119,544))"
   ]
